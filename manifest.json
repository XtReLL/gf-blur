{
  "manifest_version": 3,
  "name": "CSGF Extension",
  "description": "Неофициальное расширение для CSGF\nРазмывает аватарки пользователей на сайте",
  "version": "1.0.1",
  "icons": {
    "16": "/images/icon_16.png",
    "32": "/images/icon_32.png",
    "48": "/images/icon_48.png",
    "128": "/images/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://csgf.ru/*",
        "*://csgf.fun/*",
        "*://csgf.fan/*",
        "*://csgf.pro/*",
        "*://csgf.me/*",
        "*://csgf.live/*",
        "*://csgf.site/*"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}